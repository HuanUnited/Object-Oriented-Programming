{
    "name": "trello-clone-ssr",
    "version": "1.0.0",
    "description": "SSR Trello Clone with TypeScript and Tailwind CSS",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "build:client": "tsc --project tsconfig.client.json",
        "build:all": "npm run build && npm run build:client",
        "start": "node dist/server.js",
        "dev": "npm run build:all && npm start",
        "watch": "tsc --watch"
    },
    "keywords": [
        "trello",
        "ssr",
        "typescript"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^20.5.0",
        "typescript": "^5.1.6"
    }
}